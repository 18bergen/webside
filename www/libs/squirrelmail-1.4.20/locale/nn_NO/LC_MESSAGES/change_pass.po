#: options.php:36 setup.php:18
msgid "Change Password"
msgstr "Endra passord"

#: options.php:77
msgid "Closing Connection"
msgstr "Lukk samband"

#: options.php:137
msgid "Connecting to Password Server"
msgstr "Koplar til passordtenar"

#: options.php:141
msgid "ERROR:"
msgstr "FEIL:"

#: options.php:52
msgid "New Password"
msgstr "Nytt passord"

#: options.php:51
msgid "Old Password"
msgstr "Gamalt passord"

